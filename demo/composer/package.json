{
	"engines": {
		"composer": "^0.19.0"
	},
	"name": "stokes-network",
	"description": "Artwork Tracking Network",
	"version": "0.2.5",
	"scripts": {
		"prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/stokes-network.bna",
		"postlint": "npm run licchk",
		"licchk": "license-check-and-add",
		"test": "mocha -t 0 --recursive",
		"test-cover": "nyc npm run test"
	},
	"keywords": [
		"artwork",
		"crozier",
		"composer",
		"composer-network",
		"stokes"
	],
	"repository": {
		"type": "git",
		"url": "https://github.ibm.com/Project-Stokes/stokes-mvp-composer-dev.git"
	},
	"author": "Hyperledger Composer",
	"license": "Apache-2.0",
	"devDependencies": {
		"browserfs": "^1.2.0",
		"chai": "^3.5.0",
		"chai-as-promised": "^6.0.0",
		"composer-admin": "^0.19.0-0",
		"composer-cli": "^0.19.0-0",
		"composer-client": "^0.19.0-0",
		"composer-common": "^0.19.0-0",
		"composer-connector-embedded": "^0.19.0-0",
		"eslint": "^3.19.0",
		"istanbul": "^0.4.5",
		"jsdoc": "^3.5.5",
		"license-check-and-add": "~2.3.0",
		"mkdirp": "^0.5.1",
		"mocha": "^3.2.0",
		"moment": "^2.17.1",
		"nyc": "^11.0.2"
	},
	"license-check-and-add-config": {
		"folder": ".",
		"license": "LICENSE.txt",
		"exact_paths_method": "EXCLUDE",
		"exact_paths": [
			"composer-logs",
			"dist",
			"node_modules",
			"out",
			".git"
		],
		"file_type_method": "EXCLUDE",
		"file_types": [
			".yml"
		],
		"insert_license": false,
		"license_formats": {
			"js|cto|acl|qry": {
				"prepend": "/*",
				"append": " */",
				"eachLine": {
					"prepend": " * "
				}
			},
			"md": {
				"file": "../../markdown-license.txt"
			}
		}
	}
}
